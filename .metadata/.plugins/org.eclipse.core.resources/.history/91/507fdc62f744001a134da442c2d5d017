package reflection;

public class WorkerValue {
	public static int POSITION_MANAGET = 0;
	public static int POSITION_ASSISTANT = 1;
	public static int POSITION_EMPLOYEE = 2;
	
	private String name;
	private int position;
	
	public WorkerValue() {
		name = null;
		position = Integer.MAX_VALUE;
	}
}
